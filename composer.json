{
    "name": "jan-dolata/translations_editor",
    "type": "library",
    "description": "Translations editor",
    "keywords": [
        "jan-dolata",
        "translations_editor"
    ],
    "homepage": "https://github.com/jan-dolata/translations_editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Dolata",
            "email": "jan.dolata.gd@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JanDolata\\TranslationsEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JanDolata\\TranslationsEditor\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
